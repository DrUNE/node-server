{
  "name": "node-server",
  "version": "0.0.1",
  "description": "Barebone node Restful server",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DrUNE/node-server.git"
  },
  "keywords": [
    "node",
    "minimal"
  ],
  "author": "DrUNE",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DrUNE/node-server/issues"
  },
  "homepage": "https://github.com/DrUNE/node-server#readme",
  "devDependencies": {
    "@types/node": "^10.12.10",
    "nodemon": "^1.18.6",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {}
}
